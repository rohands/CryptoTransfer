<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rohan.securelibrary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.rohan.securelibrary.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.rohan.securelibrary.Registration" />
        <activity android:name="com.example.rohan.securelibrary.Login" />
        <activity android:name="com.example.rohan.securelibrary.Home" />
        <activity android:name="com.example.rohan.securelibrary.search.SearchFunctionality" />
        <activity android:name="com.example.rohan.securelibrary.Transaction" />
        <activity android:name="com.example.rohan.securelibrary.RenewBalance" />
        <activity android:name="com.example.rohan.securelibrary.CallbackImplementor" />
        <activity android:name="com.example.securetransfer.generator.Generator" />
        <activity android:name="com.example.securetransfer.scanner.Scanner" />
        <activity android:name="com.example.securetransfer.bluetooth.BluetoothSend" />
        <activity android:name="com.example.securetransfer.bluetooth.BluetoothReceive" />
        <activity android:name="com.example.securetransfer.buyer.BuyerIntroduction" />
        <activity android:name="com.example.securetransfer.buyer.BuyerTransaction" />
    </application>

</manifest>
